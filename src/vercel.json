{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/process-documents",
        "dest": "/api/process-documents"
      }
    ],
    "functions": {
      "api/process-documents.js": {
        "memory": 128,
        "maxDuration": 10
      }
    },
    "crons": [
      {
        "path": "/api/process-documents",
        "schedule": "*/10 * * * *" 
      }
    ]
  }
  